{
  "name": "residence-crawler",
  "version": "0.0.1",
  "main": "handler.js",
  "scripts": {
    "format": "prettier --config .prettierrc --write \"src/**/*.{ts,js}\"",
    "docs": "typedoc --tsconfig tsconfig.json --entryPoints src/**/*.ts --out docs/",
    "ts-node": "ts-node",
    "crawler": "ts-node src/cli.ts",
    "sequelize": "sequelize",
    "sequelize:db:create": "sequelize db:create --charset utf8mb4 --collate utf8mb4_unicode_ci"
  },
  "author": "TakuKobayashi",
  "private": true,
  "dependencies": {
    "mysql2": "^3.9.7",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "commander": "^12.0.0",
    "documentation": "^14.0.3",
    "dotenv": "^16.4.5",
    "fast-glob": "^3.3.2",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.27.5",
    "typescript": "^5.4.5",
    "typings": "^2.1.1"
  },
  "license": "MIT"
}
